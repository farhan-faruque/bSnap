<li class="dropdown">
  <%= link_to '#',class: "dropdown-toggle", role: "button", :data => {:toggle=>"dropdown"}, 'aria-expanded' => false do %>
      <span id="span"><%= current_user.user_name %></span>
  <% end %>

  <ul class="dropdown-menu" role="menu">
    <li><%= link_to 'Profile',user_profile_path(current_user) %></li>
    <li><%= link_to 'Ads',user_ads_path(current_user)%></li>

    <li><%= link_to 'Favourites',user_favorites_path(current_user)%></li>
    <li class="divider"></li>
    <li> <%= link_to 'Signout', destroy_user_session_path, html_options = { method: :delete} %></li>
  </ul>
</li>
